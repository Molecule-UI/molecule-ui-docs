{
  "name": "@molecule-ui/system",
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.0.3-alpha.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "update": "yarn upgrade @molecule-ui/icons@latest",
    "develop": "env-cmd gatsby develop",
    "dev-m": "env-cmd gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.16",
    "@mdx-js/react": "^1.6.16",
    "@moleculeui/icons": "^1.1.0-beta.0",
    "@moleculeui/react": "^1.1.2-beta.3",
    "@storybook/addon-controls": "^6.4.22",
    "env-cmd": "^10.1.0",
    "gatsby": "^2.24.26",
    "gatsby-image": "^2.4.14",
    "gatsby-plugin-mdx": "^1.2.34",
    "gatsby-plugin-page-creator": "^2.3.22",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.26",
    "gatsby-source-filesystem": "^2.3.24",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^2.5.12",
    "node-sass": "^4.14.1",
    "prism-react-renderer": "^1.2.0",
    "react": "^16.12.0",
    "react-code-blocks": "^0.0.8",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "babel-loader": "^8.2.5",
    "prettier": "2.0.5"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "gitHead": "ae8b5229e708f9870542db8c74ec0ce763e0b78e"
}
